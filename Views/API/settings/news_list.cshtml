@using SL.Util
@{
    RequestUtil req = new RequestUtil();

    string name = req.String("name");
    var settingUtil = new SL.Util.SettingUtil<SL.Web.Model.HtmlBlock>(name + "News");
    var data = settingUtil.Get().OrderByDescending(a => a.Sort);

    Json.Write(new { success = true, data = data, name= name }, Output);
}
